{
  "expo": {
    "name": "OwnerDekhle",
    "slug": "OwnerDekhle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/owner-dekhle-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.ownerdekhle.ownerdekhle",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/owner-dekhle-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/owner-dekhle-splash.jpg"
    },
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow access to camera."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "54fbf640-56ab-4310-9da4-abbf571ab139"
      }
    }
  }
}
